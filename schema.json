{
  "database_schema": {
    "tenants": {
      "id": "uuid primary key",
      "name": "varchar not null",
      "slug": "varchar unique not null",
      "domain": "varchar",
      "logo_url": "text",
      "settings": "jsonb",
      "created_at": "timestamp with time zone default now()",
      "updated_at": "timestamp with time zone default now()"
    },
    "users": {
      "id": "uuid primary key",
      "tenant_id": "uuid not null references tenants(id)",
      "email": "varchar unique not null",
      "full_name": "varchar not null",
      "avatar_url": "text",
      "role": "enum('admin', 'organizer', 'user') default 'user'",
      "created_at": "timestamp with time zone default now()",
      "updated_at": "timestamp with time zone default now()"
    },
    "events": {
      "id": "serial primary key",
      "tenant_id": "uuid not null references tenants(id)",
      "category": "varchar not null",
      "title": "varchar not null",
      "description": "text",
      "date": "varchar not null",
      "time": "varchar not null",
      "location": "varchar not null",
      "organizer": "varchar not null",
      "organizer_id": "uuid not null references users(id)",
      "image_url": "text not null",
      "vip_tickets": "integer default 0",
      "is_trending": "boolean default false",
      "tickets_sold": "integer default 0",
      "tickets_remaining": "integer default 0",
      "price": "decimal(10,2) not null",
      "vip_price": "decimal(10,2) not null",
      "latitude": "decimal(10,8)",
      "longitude": "decimal(11,8)",
      "venue_details": "jsonb",
      "created_at": "timestamp with time zone default now()",
      "updated_at": "timestamp with time zone default now()"
    },
    "tickets": {
      "id": "uuid primary key",
      "tenant_id": "uuid not null references tenants(id)",
      "event_id": "integer not null references events(id)",
      "user_id": "uuid not null references users(id)",
      "ticket_type": "enum('standard', 'vip') not null",
      "ticket_number": "varchar unique not null",
      "price": "decimal(10,2) not null",
      "status": "enum('pending', 'confirmed', 'cancelled') default 'pending'",
      "qr_code": "varchar not null",
      "payment_method": "enum('stripe', 'mpesa') not null",
      "payment_id": "varchar",
      "created_at": "timestamp with time zone default now()",
      "updated_at": "timestamp with time zone default now()"
    },
    "saved_events": {
      "id": "uuid primary key",
      "tenant_id": "uuid not null references tenants(id)",
      "user_id": "uuid not null references users(id)",
      "event_id": "integer not null references events(id)",
      "created_at": "timestamp with time zone default now()",
      "unique_constraint": "unique(user_id, event_id)"
    }
  },
  "api_endpoints": {
    "GET /api/events": "Filter and paginate events",
    "POST /api/events": "Create new event (organizer/admin only)",
    "GET /api/events/:id": "Get event details",
    "PUT /api/events/:id": "Update event (owner/admin only)",
    "DELETE /api/events/:id": "Delete event (owner/admin only)",
    "POST /api/events/:id/save": "Save/unsave event for user",
    "POST /api/tickets/buy": "Purchase tickets",
    "GET /api/tickets/my": "Get user's tickets",
    "GET /api/users/profile": "Get user profile",
    "PUT /api/users/profile": "Update user profile"
  },
  "sample_data": {
